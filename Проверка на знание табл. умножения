#include <iostream>
using namespace std;

int main() {
    int a;
    int b;
    int c;
    
    cout << "Введите число a: " << endl;
    cin >> a;
    
    cout << "Введите число b: " << endl;
    cin >> b;
    
    cout << "Введите результат умножения a и b: " << endl;
    cin >> c;

    if (c == a * b) {
        cout << "Верно!" << endl;
    }
    else {
        cout << "Не верно! Попробуйте еще." << endl;
    }
    return 0;
}
